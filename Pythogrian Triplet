#include<bits/stdc++.h>
using namespace std;

int main(){
    int a,b,c,x,y,z;
    cin>>x>>y>>z;
    int temp = max(a,b);
    int tem2 = max(temp, c);
    a = tem2;
    if(a^2 == (b^2 + c^2)){
        cout<<"Yes its a triplet";
    }
    else{
        cout<<"Its not a triplet";
    }
    
    return 0;
}
