#include<bits/stdc++.h>
using namespace std;
int main(){
    int i=0;
    // for loop
    for (i=0 ; i < 10; i++){
        cout << i ;   
    }
    // while loop
    while (i<10)
    {
        cout << i;
        i++;
    }

    //do while loop
    do{
        cout << i;
        i++;
    }while(i<10);
    
    
return 0;
}